  "bugs": {
    "url": "https://github.com/pedrojlazevedo/twitch-chatgpt/issues"
  },
  "homepage": "https://github.com/Araxielfenix/twitch-rodentbot#readme",
  "dependencies": {
    "cron": "^3.1.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "openai": "^4.24.7",
    "promisify": "^0.0.3",
    "request": "^2.88.2",
    "tmi.js": "^1.8.5",
    "dotenv": "^16.0.3",
    "discord.js": "^14.14.1",
    "ws": "^8.14.2",
    "concurrently": "^9.0.0"
  },
